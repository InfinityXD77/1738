local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local folder = workspace:WaitForChild("ActiveBrainrots"):WaitForChild("Secret")

local Window = Rayfield:CreateWindow({
    Name = "1738 Hub",
    Icon = 0,
    LoadingTitle = "Loading...",
    LoadingSubtitle = "made by yt-@Opgamer73431",
    ShowText = "1738",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "1738 Hub"
    },
    KeySystem = false
})

local Tab = Window:CreateTab("Main","box")
Tab:CreateSection("Spawn Alerts")

local function notify(text)
    Rayfield:Notify({
        Title = "Spawn Alert",
        Content = text,
        Duration = 5,
        Image = "bell-ring"
    })
end

local function checkName(name)
    local lower = string.lower(name)
    if string.find(lower, "67") then notify("rainbow 67 spawned") end
    if string.find(lower, "frago") then notify("frago la la la spawned") end
    if string.find(lower, "aura") then notify("aura farma spawned") end
    if string.find(lower, "eek") then notify("eek eek eek sahur spawned") end
end

for _, obj in ipairs(folder:GetChildren()) do
    local inner = obj:FindFirstChildWhichIsA("Model")
    if inner then
        checkName(inner.Name)
    end
end

folder.ChildAdded:Connect(function(obj)
    local inner = obj:FindFirstChildWhichIsA("Model")
    if inner then
        checkName(inner.Name)
    end
end)

Rayfield:Notify({
    Title = "1738 Hub Loaded!!",
    Content = "Enjoy....",
    Duration = 6.5,
    Image = "bell-ring"
})
